<!DOCTYPE html>
<html>
<head>
  <title>DontTalk</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <h1>Searching for Words </h1>
  <%= @words.each do |row| %>
  <%= row["state"] %>
  <%= row["count"] %>
  <% end %>

  <h1>Police-related fatalities</h1>
  <%= @fatalities.each do |row| %>
  <%= row["name"] %>
  <%= row["age"] %>
  <% end %>

</body>
</html>
